<%
createStarRating = function(Award) {
    let rating

    if (Award === "Bib Gourmand") {
        return "Bib Gourmand"
    } else {
        rating = parseInt(Award.charAt(0))
    }

    let starRating = "";

    for (let i = 0; i < rating; i++) {
        starRating += "★";
    }
    for (let i = 0; i < 3 - rating; i++) {
        starRating += "☆";
    }

    return starRating;
}
%>