<div id=<%=link.id%> class="mt-3">
    <h6 class="mb-3">
        <%=link.heading%>
    </h6>
    <div>
        <% for (const key in diningCriteria) { %>
            <% if (typeof user[diningCriteria[key]] === "undefined") {%>
                <p class="text-center w-100 mt-3 mb-5 btn-no-preference">Rank your Dining Criteria to enjoy our service!</p>
                <% break; %>
                    <% } %>
                        <div class="d-flex justify-content-between mb-2 text-uppercase">
                            <span class="btn-success btn-sm text-left" style="width: min(45%,150px)">
                                <%=diningCriteria[key]%>
                            </span>
                            <span class="btn-info btn-sm w-30 mx-3 text-white px-3">
                                <%=user[diningCriteria[key]]%>
                            </span>
                        </div>
                        <% }%>
    </div>
</div>